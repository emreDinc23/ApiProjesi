@model List<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hizmet Başlığı</th>
        <th>Sil</th>
        <th>Güncelle</th>
        
       
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ServiceID</td>
            <td>@x.Title</td>

            <td><a href="/Service/ServiceDelete/@x.ServiceID" class="btn mb-1 btn-rounded btn-outline-danger"> Sil</a></td>
            <td><a href="/Service/UpdateService/@x.ServiceID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
          
            
        </tr>

    }

</table>
<a href="/Service/AddService" class="btn mb-1 btn-rounded btn-outline-primary">Yeni Personel Girişi</a>



